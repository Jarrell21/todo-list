{
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": "off",
      "eqeqeq":"off",
      "camelcase":"off",
      "prefer-const":"warn",
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 80,
          "tabWidth": 4,
          "endOfLine": "auto"
        }
      ]
    }
  }